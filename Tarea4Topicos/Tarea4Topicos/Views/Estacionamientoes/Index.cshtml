@model IEnumerable<Tarea4Topicos.Models.Estacionamiento>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaIngreso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaSalida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TarifaBasica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ganancia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mantenimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImpuestoVenta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalAPagar)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaIngreso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaSalida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TarifaBasica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ganancia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mantenimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImpuestoVenta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalAPagar)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EstacionamientoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EstacionamientoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EstacionamientoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
